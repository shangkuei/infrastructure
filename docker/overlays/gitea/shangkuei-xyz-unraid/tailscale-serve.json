{
  "TCP": {
    "443": {
      "HTTPS": true
    },
    "22": {
      "TCPForward": "gitea:22"
    }
  },
  "Web": {
    "${TS_CERT_DOMAIN}:443": {
      "Handlers": {
        "/": {
          "Proxy": "http://gitea:3000"
        }
      }
    }
  },
  "AllowFunnel": {
    "${TS_CERT_DOMAIN}:443": false
  }
}
